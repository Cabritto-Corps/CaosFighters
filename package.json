{
    "name": "poo",
    "main": "expo-router/entry",
    "version": "1.0.0",
    "scripts": {
        "start": "expo start -c",
        "reset-project": "node ./scripts/reset-project.js",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
        "backend:dev": "nodemon --config nodemon-backend.json",
        "backend:start": "cd backend && ts-node server.ts",
        "backend:build": "tsc --project tsconfig-backend.json",
        "backend:test": "jest --config jest-backend.config.js",
        "backend:lint": "eslint backend --ext .ts --config .eslintrc-backend.json",
        "dev:full": "concurrently \"pnpm run backend:dev\" \"pnpm run start\""
    },
    "dependencies": {
        "@expo/ngrok": "^4.1.3",
        "@expo/vector-icons": "^15.0.2",
        "@react-native-async-storage/async-storage": "2.2.0",
        "@react-navigation/bottom-tabs": "^7.4.0",
        "@react-navigation/elements": "^2.3.8",
        "@react-navigation/native": "^7.1.8",
        "@react-navigation/native-stack": "^7.3.16",
        "@supabase/supabase-js": "^2.54.0",
        "add": "^2.0.6",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "expo": "~54.0.13",
        "expo-av": "~16.0.7",
        "expo-blur": "~15.0.7",
        "expo-constants": "~18.0.9",
        "expo-font": "~14.0.9",
        "expo-haptics": "~15.0.7",
        "expo-image": "~3.0.9",
        "expo-linear-gradient": "^15.0.7",
        "expo-linking": "~8.0.8",
        "expo-location": "~19.0.7",
        "expo-notifications": "~0.32.12",
        "expo-router": "~6.0.12",
        "expo-splash-screen": "~31.0.10",
        "expo-status-bar": "~3.0.8",
        "expo-symbols": "~1.0.7",
        "expo-system-ui": "~6.0.7",
        "expo-web-browser": "~15.0.8",
        "express": "^5.1.0",
        "pnpm": "^10.18.3",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-native": "0.81.4",
        "react-native-gesture-handler": "~2.28.0",
        "react-native-reanimated": "~4.1.3",
        "react-native-safe-area-context": "5.6.1",
        "react-native-screens": "~4.16.0",
        "react-native-web": "~0.21.2",
        "react-native-webview": "13.15.0",
        "zustand": "^5.0.7"
