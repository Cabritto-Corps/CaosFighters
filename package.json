        "@react-navigation/native-stack": "^7.3.25",
        "@supabase/supabase-js": "^2.54.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "expo": "~53.0.20",
        "expo-blur": "~14.1.5",
        "expo-constants": "~17.1.7",
        "expo-font": "~13.3.2",
        "expo-haptics": "~14.1.4",
        "expo-image": "~2.4.0",
        "expo-linking": "~7.1.7",
        "expo-location": "~18.1.6",
        "expo-notifications": "~0.31.4",
